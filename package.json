{
  "name": "deep-diff-esm",
  "description": "Javascript utility for calculating deep difference, capturing changes, and applying changes across objects; for nodejs and the browser.",
  "version": "1.0.2",
  "license": "MIT",
  "keywords": [
    "diff",
    "difference",
    "compare",
    "change-tracking"
  ],
  "author": "Timur Atalay",
  "repository": {
    "type": "git",
    "url": "git://github.com/bornova/deep-diff-esm.git"
  },
  "type": "module",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "jsdelivr": "./dist/browser/deep-diff.min.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prebuild": "npm test",
    "build": "rollup -c && npm run types",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "pretest": "npm run prettier && npm run lint",
    "test": "mocha --recursive",
    "test:coverage": "c8 mocha --recursive",
    "test:watch": "mocha --recursive --watch --parallel --reporter min",
    "types": "tsc"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "rollup": "^4.57.0",
    "typescript": "^5.9.3"
  }
}
